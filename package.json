{
  "name": "ai-chatbot",
  "version": "3.1.0",
  "private": true,
  "scripts": {
    "dev": "lsof -ti:3333 | xargs kill -9 2>/dev/null; next dev --turbo --port 3333",
    "build": "tsx lib/db/migrate && next build",
    "start": "next start",
    "lint": "next lint && biome lint --write --unsafe",
    "lint:fix": "next lint --fix && biome lint --write --unsafe",
    "format": "biome format --write",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "npx tsx lib/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "supabase:migrate:local": "supabase db push --local",
    "supabase:migrate:prod": "supabase db push",
    "migrate:local": "pnpm db:migrate && pnpm supabase:migrate:local",
    "migrate:prod": "pnpm db:migrate && pnpm supabase:migrate:prod",
    "db:reset:local": "./scripts/reset-database-with-order.sh",
    "db:reset:prod": "echo '⚠️  WARNING: This will reset the production database! Press Ctrl+C to cancel.' && sleep 5 && supabase db reset && pnpm migrate:prod",
    "db:types": "npx supabase gen types typescript --local 2>/dev/null | grep -v '^Connecting to db' > lib/supabase/types.ts",
    "test": "export PLAYWRIGHT=True && pnpm exec playwright test"
  },
